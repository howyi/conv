{
    "name": "howyi/conv",
    "description": "MySQL migration query auto generate from schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":  "Hayashi Takuya",
            "email": "qrbys@outlook.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laminaria/conv-test-suite"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php":                      ">=7.1",
        "symfony/console":          ">=2.0",
        "symfony/yaml":             ">=2.0",
        "howyi/evi":                "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Laminaria\\Conv\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit":           "^6.2",
        "symfony/var-dumper":        "^3.3",
        "satooshi/php-coveralls":    "^1.0",
        "phpspec/prophecy":          "^1.7",
        "phpstan/phpstan":           "^0.8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "howyi/retort":              "~1.0.0",
        "laminaria/conv-test-suite": "dev-master"
    },
    "bin": ["bin/conv"]
}
